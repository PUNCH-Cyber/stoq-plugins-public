[Core]
Name = decompress
Module = decompress

[Documentation]
Author = Marcus LaFerrera
Version = 3.0.3
Website = https://github.com/PUNCH-Cyber/stoq-plugins-public
Description = Extract content from a multitude of archive formats

[options]
min_stoq_version = 3.0.0

# Passwords to iterate over when decompressing a file
# Default: -
# passwords = -,infected,password

# Maximum size of payloads to process (original compressed file and extracted files)
# Default: 50MB
# maximum_size = 50000000

# How long (in seconds) to wait for decompression to finish
# Default: 45
# timeout = 45
#
# Always dispatch extracted files to these plugins
# Default: Empty List
# always_dispatch = yara, exif
#
# Always erchive extracted files
# Default: True
# archive_extracted = True
